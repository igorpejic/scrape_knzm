      <span on-validate="quantityValidated(valid)" model="product" inc="" product-quantity-small="" class="ng-isolate-scope"><span ng-class="{ fm_error: !valid }" class="iblock relative no_selection">
  <span ng-click="decQuantity()" ng-show="show.dec" class="ico ico_minus ico_minus_s js_minus ng-hide">-</span>
  <input type="text" ng-change="updateQuantity()" ng-model="quantity" class="fm_xs txt_c item_nr ng-pristine ng-valid" />
  <span ng-click="incQuantity()" ng-show="show.inc" class="ico ico_plus ico_plus_s js_plus">+</span>
</span></span>
      <span bo-bind="product.salesUnitOfMeasure | capitalize" class="fine">Ko</span>
      <button ng-bind="toCartBtnText" bo-disabled="!canAdd()" ng-click="toCart()" class="btn btn_s btn_alt btn_buy ng-binding" ng-class="{'btn_neutral':toCartBtnAnimated}" href="">U košaricu</button>
      <div ng-class="{'txt_l': view == 'grid'}" class="small clr_dist mt_xxs txt_l">
        <!-- boIf: product.catchWeight == 1 -->
        <!-- boIf: product.catchWeight != 1 --><span bo-if="product.catchWeight != 1" class="ng-scope"> </span>
        <!-- boIf: product.showMaxOrderQty() -->
      </div>
    </div>

    <div ng-bind-template="U košarici imate 0 Ko." ng-class="{'right mr_s': view != 'grid'}" ng-show="quantityInCart() != 0" class="quantity_info txt_c mv_xs em ng-binding ng-hide">U košarici imate 0 Ko.</div>

    <!-- ngIf: product.sliceable && !disableSliceable() -->

    <!-- boIf: product.offer -->
  </div>
  <!-- boIf: !product.active -->
</div></div>

  <div>
    <a ng-click="toggleFavorite()" class="btn_favorite" href="">
      <span bo-bind="'fronted.common.add_to_favorites' | translate" class="structural">fronted.common.add_to_favorites</span>
      <span ng-class="{ active: product.favorite }" class="ico_fav"></span>
    </a>
  </div>
</div>
            </div>

</div>
          </div>
        </li><!-- end ngRepeat: product in products -->
      </ul><!-- end ngIf: products.length > 0 -->

      <!--
      <div ng-show="showLoadMore">
